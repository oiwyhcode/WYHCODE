
#ifndef USERHH_TRACE_PID_H_
#define USERHH_TRACE_PID_H_
#include "main.h"


typedef struct {
    float Kp;
    float Ki;
    float Kd;

    float Error;
    float LastError;
    float Integral;

    float target_val; // 目标值，对于循迹，通常是0
    float output_val;   // PID计算输出
} PID_Trace;

#endif /* USERHH_TRACE_PID_H_ */
